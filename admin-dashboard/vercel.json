{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "pnpm run build",
  "installCommand": "pnpm install --no-frozen-lockfile",
  "framework": "sveltekit",
  "devCommand": "pnpm run dev",
  "outputDirectory": ".svelte-kit/output",
  "functions": {
    "src/routes/**/*": {
      "runtime": "nodejs18.x",
      "maxDuration": 30
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}