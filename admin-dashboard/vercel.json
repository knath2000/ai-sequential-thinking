{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "pnpm run build",
  "installCommand": "pnpm install --no-frozen-lockfile",
  "framework": "sveltekit",
  "outputDirectory": ".svelte-kit/output",
  "functions": {
    "src/routes/**/*": {
      "runtime": "nodejs22.x",
      "maxDuration": 30,
      "memory": 1024,
      "includeFiles": "node_modules/**"
    }
  },
  "regions": ["iad1"],
  "env": {
    "NODE_ENV": "production"
  }
}